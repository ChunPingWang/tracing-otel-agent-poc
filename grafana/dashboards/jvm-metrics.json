{
  "uid": "jvm-metrics",
  "title": "JVM Metrics",
  "tags": ["otel", "ecommerce", "jvm"],
  "timezone": "browser",
  "schemaVersion": 39,
  "editable": true,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "query",
        "datasource": {
          "type": "prometheus"
        },
        "query": "label_values(process_runtime_jvm_memory_usage_bytes, service_name)",
        "definition": "label_values(process_runtime_jvm_memory_usage_bytes, service_name)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Heap Memory",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 8
      },
      "datasource": {
        "type": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(process_runtime_jvm_memory_usage_bytes{type=\"heap\", service_name=~\"$service\"}) by (service_name)",
          "legendFormat": "{{service_name}} used"
        },
        {
          "refId": "B",
          "expr": "sum(process_runtime_jvm_memory_committed_bytes{type=\"heap\", service_name=~\"$service\"}) by (service_name)",
          "legendFormat": "{{service_name}} committed"
        },
        {
          "refId": "C",
          "expr": "sum(process_runtime_jvm_memory_limit_bytes{type=\"heap\", service_name=~\"$service\"}) by (service_name)",
          "legendFormat": "{{service_name}} limit"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Non-Heap Memory",
      "gridPos": {
        "x": 0,
        "y": 9,
        "w": 24,
        "h": 8
      },
      "datasource": {
        "type": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(process_runtime_jvm_memory_usage_bytes{type=\"non_heap\", service_name=~\"$service\"}) by (service_name)",
          "legendFormat": "{{service_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "GC Count Rate",
      "gridPos": {
        "x": 0,
        "y": 18,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(process_runtime_jvm_gc_duration_seconds_count{service_name=~\"$service\"}[$__rate_interval])) by (service_name, gc)",
          "legendFormat": "{{service_name}} {{gc}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "GC Duration Rate",
      "gridPos": {
        "x": 12,
        "y": 18,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(process_runtime_jvm_gc_duration_seconds_sum{service_name=~\"$service\"}[$__rate_interval])) by (service_name, gc)",
          "legendFormat": "{{service_name}} {{gc}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Thread Count",
      "gridPos": {
        "x": 0,
        "y": 27,
        "w": 24,
        "h": 8
      },
      "datasource": {
        "type": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "process_runtime_jvm_threads_count{service_name=~\"$service\"}",
          "legendFormat": "{{service_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Memory Pool Usage",
      "gridPos": {
        "x": 0,
        "y": 36,
        "w": 24,
        "h": 8
      },
      "datasource": {
        "type": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "process_runtime_jvm_memory_usage_bytes{service_name=~\"$service\"}",
          "legendFormat": "{{service_name}} — {{pool}} ({{type}})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["last", "mean"]
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Class Loading",
      "gridPos": {
        "x": 0,
        "y": 45,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "process_runtime_jvm_classes_current_loaded{service_name=~\"$service\"}",
          "legendFormat": "{{service_name}} — loaded"
        },
        {
          "refId": "B",
          "expr": "rate(process_runtime_jvm_classes_unloaded{service_name=~\"$service\"}[$__rate_interval])",
          "legendFormat": "{{service_name}} — unloaded/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["last", "mean"]
        }
      }
    }
  ]
}
